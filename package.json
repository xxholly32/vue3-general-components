{
  "name": "vue3-general-components",
  "version": "0.0.1",
  "main": "lib/index.esm.js",
  "module": "lib/index.esm.js",
  "author": {
    "name": "xiangxiao",
    "email": "xxholly32@163.com"
  },
  "license": "MIT",
  "files": [
    "lib"
  ],
  "description": "repo for vue3 components",
  "repository": {
    "type": "git",
    "url": "https://github.com/xxholly32/vue3-general-components"
  },
  "bugs": {
    "url": "https://github.com/xxholly32/vue3-general-components/issues"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "release": "standard-version && npm run cz:changelog",
    "cz:changelog": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.37",
    "@vitejs/plugin-vue": "^1.2.5",
    "@vitejs/plugin-vue-jsx": "^1.1.6",
    "@vue/babel-plugin-jsx": "^1.0.6",
    "@vue/compiler-sfc": "^3.0.5",
    "conventional-changelog-cli": "^2.0.34",
    "standard-version": "^8.0.0",
    "typescript": "^4.3.2",
    "vite": "^2.4.2",
    "vite-plugin-md": "^0.6.7",
    "vite-plugin-pages": "^0.9.3",
    "vite-plugin-vue-layouts": "^0.3.1",
    "vite-plugin-windicss": "^1.2.4",
    "vue-router": "^4.0.5",
    "vue-tsc": "^0.0.24",
    "windicss": "^3.1.5"
  },
  "browserslist": [
    "defaults"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
